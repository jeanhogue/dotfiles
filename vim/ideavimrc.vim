" Few settings specific for Intellij IdeaVim plugin
source ~/dotfiles/vim/vimrc.vim
let mapleader = " "
set easymotion
set ideajoin
set clipboard+=ideaput

nnoremap <C-a> 0
nnoremap <C-e> $

nnoremap <leader>s <Plug>(easymotion-s)
